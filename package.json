{
  "name": "avr-app",
  "type": "module",
  "version": "1.0.4",
  "author": "Giuseppe Careri <info@gcareri.com> (https://gcareri.com)",
  "description": "AdminJS application for AgentVoiceResponse.com",
  "scripts": {
    "start:dev": "nodemon",
    "start": "node dist/index.js",
    "build": "tsc",
    "build:watch": "tsc -w",
    "build:theme": "npm clean && tsc && npm copy-theme",
    "copy-theme": "copyfiles -u 1 src/themes/**/theme.bundle.js src/themes/**/*.css dist/",
    "dc:up": "docker compose up -d --build",
    "dc:down": "docker compose down"
  },
  "license": "MIT",
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@adminjs/logger": "^5.0.1",
    "@adminjs/sequelize": "^4.1.1",
    "@adminjs/themes": "^1.0.1",
    "adminjs": "^7.8.15",
    "ari-client": "^2.2.0",
    "connect-session-sequelize": "^7.1.7",
    "cors": "^2.8.5",
    "dockerode": "^4.0.4",
    "dotenv": "^16.4.7",
    "express-session": "^1.18.1",
    "mysql2": "^3.12.0",
    "react-syntax-highlighter": "^15.6.1",
    "sequelize": "^6.37.5",
    "tar-fs": "^3.0.8",
    "tar-stream": "^3.1.7"
  },
  "devDependencies": {
    "@types/ari-client": "^2.2.13",
    "@types/cors": "^2.8.13",
    "@types/express-session": "^1.17.7",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "copyfiles": "^2.4.1",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.1.9",
    "prettier": "^2.8.6",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2"
  }
}
